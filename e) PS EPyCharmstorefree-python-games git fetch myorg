[31m-[m
[31m-    clues = [][m
[31m-[m
[31m-    for index in range(digits):[m
[31m-        if guess[index] == number[index]:[m
[31m-            clues.append('fermi')[m
[31m-        elif guess[index] in number:[m
[31m-            clues.append('pico')[m
[31m-[m
[31m-    shuffle(clues)[m
[31m-[m
[31m-    if len(clues) == 0:[m
