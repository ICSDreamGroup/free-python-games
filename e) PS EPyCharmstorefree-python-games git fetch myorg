[31m-    else:[m
[31m-        print(' '.join(clues))[m
[31m-[m
[31m-    counter += 1[m
[31m-[m
[31m-    if guess == number:[m
[31m-        print('You got it!')[m
[31m-        break[m
[31m-[m
[31m-    if counter > guesses:[m
[31m-        print('You ran out of guesses. The answer was', number)[m
[31m-        break[m
